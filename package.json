{
  "name": "nextjs-to-aws",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "db:generate": "drizzle-kit generate:pg --schema=./src/server/db/schema.ts --out=./src/server/db/migrations",
    "db:migrate": "tsx src/server/db/migrate.ts",
    "db:push": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio",
    "dev": "concurrently -n \"lint,next\" -c \"bgBlue.bold,bgBlack.bold\" \"tsc --noEmit --incremental --watch\" \"NODE_ENV=development && next dev\"",
    "lint": "next lint",
    "start": "next start"
  },
